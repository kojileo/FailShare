{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start:dev": "cross-env EXPO_PUBLIC_ENVIRONMENT=development expo start",
    "start:staging": "cross-env EXPO_PUBLIC_ENVIRONMENT=staging expo start",
          "start:prod": "cross-env EXPO_PUBLIC_ENVIRONMENT=production expo start",
      "preview:dev": "expo start --dev-client",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "echo \"No linting configured\" && exit 0",
    "type-check": "tsc --noEmit",
    "build:web": "expo export --platform web",
    "build:web:dev": "cross-env EXPO_PUBLIC_ENVIRONMENT=development expo export --platform web",
    "build:web:staging": "cross-env EXPO_PUBLIC_ENVIRONMENT=staging expo export --platform web",
    "build:web:prod": "cross-env EXPO_PUBLIC_ENVIRONMENT=production expo export --platform web",
    "server:start": "node server.js",
    "server:dev": "npm run build:web:dev && npm run server:start",
    "server:prod": "npm run build:web:prod && npm run server:start"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "dotenv": "^17.2.0",
    "expo": "~53.0.17",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-status-bar": "~2.2.3",
    "express": "^4.21.2",
    "firebase": "^11.10.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.20.0",
    "swr": "^2.3.4",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/cli": "latest",
    "@types/react": "~19.0.10",
    "cross-env": "^7.0.3",
    "typescript": "~5.8.3"
  },
  "private": true
}
